KERNEL=="ttyUSB*", ATTRS{idVendor}=="10c4", ATTRS{idProduct}=="ea60", MODE:="0666", GROUP:="dialout",  SYMLINK+="rplidar"
ACTION=="add", ENV{DEVNAME}=="/dev/ttyUSB*", ATTRS{idVendor}=="10c4", ATTRS{idProduct}=="ea60", RUN+="/bin/systemctl start tianbot_racecar_rplidar.service"
ACTION=="remove", ENV{DEVNAME}=="/dev/ttyUSB*", ENV{ID_VENDOR_ID}=="10c4", ENV{ID_MODEL_ID}=="ea60", RUN+="/bin/systemctl stop tianbot_racecar_rplidar.service"
